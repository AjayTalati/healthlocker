<%= render Healthlocker.CareTeamView, "menu.html", assigns %>

<div class="pt6">
  <%= if @current_user == @service_user do %>
    <h3>Your care team</h3>
  <%= else %>
    <h3><%= Healthlocker.ComponentView.full_name(@service_user) %>'s care team</h3>
  <% end %>

  <%= for member <- @care_team do %>
    <div class="w-100 hl-bg-grey pa3 mv3 br3">
      <p class="b di"><%= Healthlocker.ComponentView.epjs_full_name(member) %></p>
    </div>
  <% end %>
</div>
