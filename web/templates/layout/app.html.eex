        <a href="/">Home</a>
        <a href="#">About</a>
        <a href="#">Log in</a>
        <a href="#">Get support</a>
      </nav>

      <main role="main">
        <%= render @view_module, @view_template, assigns %>
      </main>

    </div> <!-- /container -->
    <script src="<%= static_path(@conn, "/js/app.js") %>"></script>
  </body>
</html>
